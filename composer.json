{
  "name": "rustem-kaimolla/laravel-exception-reporter",
  "version": "1.0.0",
  "description": "Laravel package to report exceptions via TCP",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Rustem Kaimolla",
      "email": "rustem@exact.kz"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0 || ^11.0"
  },
  "config": {
    "platform": {
      "php": "8.1.0"
    }
  },
  "autoload": {
    "psr-4": {
      "LaravelExceptionReporter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LaravelExceptionReporter\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "orchestra/testbench": "^8.0",
    "phpunit/phpunit": "^10.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelExceptionReporter\\ExceptionReporterServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
